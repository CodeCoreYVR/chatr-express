{
  "name": "chatr-express",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "heroku-postbuild": "npm rebuild node-sass",
    "start": "nodemon ./bin/www",
    "debug": "nodemon --inspect ./bin/www",
    "db:setup": "dropdb --if-exists --echo chatr_dev && createdb --echo chatr_dev && npm run db:migrate && npm run db:seed",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "ejs": "~2.5.5",
    "express": "^4.15.3",
    "morgan": "~1.7.0",
    "multer": "^1.3.0",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^1.11.0",
    "pg": "^6.2.4",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.1.0",
    "serve-favicon": "~2.3.2",
    "sequelize-cli": "^2.7.0"
  },
  "engines": {
    "node": "8.5.0"
  }
}
